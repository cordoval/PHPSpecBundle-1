    <parameters>
        <parameter key="php_spec.generator.class">PHPSpec\PHPSpecBundle\Util\Generator</parameter>
    </parameters>

    <services>
        <service id="php_spec.generator" class="%php_spec.generator.class%"/>
    </services>
</container>
